{
	"AWSTemplateFormatVersion": "2010-09-09",
	"Description": "A template is for creating lambda from s3 buckets and EventBridge rule 'test3'",
	"Resources": {
		"LambdaFunResource": {
			"Type": "AWS::Lambda::Function",
			"Properties": {
				"FunctionName": "Lambda",
				"Code": {
					"S3Bucket": "mybuckets3341",
					"S3Key": "node-js-sample.zip"
				},
				"Description": "Used to run job",
				"Handler": "index.handler",
				"Role": "arn:aws:iam::383797601860:role/lambdarole",
				"Runtime": "nodejs18.x",
				"Timeout": 120
			}
		},
	
	    "EventRule0": {
		    "Type": "AWS::Events::Rule",
		    "Properties": {
			    "EventBusName": "default",
			    "Name": "test3",
			    "ScheduleExpression": "rate(12 hours)",
			    "State": "ENABLED"
		    }
	    }
    }
}
