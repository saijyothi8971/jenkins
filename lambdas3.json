{
	"AWSTemplateFormatVersion": "2010-09-09",
	"Description": "A template is for creating lambda from s3 buckets and EventBridge rule 'test3'",
    "Parameters": {
        "FunctionName": {
          "Type": "String",
          "Description": "The FunctionName to use within the container definition"
        }
    },
	"Resources": {
		"LambdaFunResource": {
			"Type": "AWS::Lambda::Function",
			"Properties": {
				"FunctionName": "!RefFunctionName",
				"Code": {
					"S3Bucket": "mybuckets3341",
					"S3Key": "node-js-sample.zip"
				},
				"Description": "Used to run job",
				"Handler": "index.handler",
				"Role": "arn:aws:iam::383797601860:role/lambdarole",
				"Runtime": "nodejs18.x",
				"Timeout": 120
			}
		},
	
	    "EventRule0": {
		    "Type": "AWS::Events::Rule",
		    "Properties": {
			    "EventBusName": "default",
			    "Name": "test3",
			    "ScheduleExpression": "rate(12 hours)",
			    "State": "ENABLED",
                "Targets": [{
                    "Id": "Ide0ef24fe-372c-4105-a1e2-5f6b0245872a",
                    "Arn": "arn:aws:lambda:ap-south-1:383797601860:function:Lambda"
                  }]
                
		    }
	    }

    }
}
